{
  "name": "SalesMan",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-release": "cd android && ./gradlew assembleRelease && cd ..",
    "build": "yarn run clean && yarn run tsc --",
    "clean": "rimraf build",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && pod cache clean --all;rm -rf ~/Library/Caches/CocoaPods; rm -rf Pods; rm -rf ~/Library/Developer/Xcode/DerivedData/*; pod deintegrate; pod setup; pod install;",
    "ios": "npx react-native run-ios --simulator \"iPhone X\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "npx prettier -l 'src/**/*.tsx'",
    "start": "npx react-native start",
    "start-clear": "npx react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-navigation/drawer": "^5.4.1",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "axios": "^0.19.2",
    "formik": "^2.1.4",
    "libphonenumber-js": "^1.7.50",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.11.0",
    "react-native": "^0.62.2",
    "react-native-collapsible": "^1.5.2",
    "react-native-date-picker": "^2.7.12",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-select-picker": "^0.0.3",
    "react-native-size-matters": "^0.3.0",
    "react-native-user-inactivity": "^1.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-phone-input-2": "^2.12.1",
    "react-query": "^1.2.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/react-native": "^0.62.0",
    "@types/react-native-collapsible": "^0.11.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-query": "^1.1.2",
    "@types/react-test-renderer": "16.9.2",
    "@types/yup": "^0.26.35",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-devtools": "^4.20.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "^5.0.0",
    "remove": "^0.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 70
      }
    }
  }
}
